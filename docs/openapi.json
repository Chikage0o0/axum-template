  "components": {
    "schemas": {
      "AppSettings": {
        "type": "object",
        "required": [
          "check_interval_secs",
          "welcome_message"
        ],
        "properties": {
          "check_interval_secs": {
            "type": "integer",
            "format": "int64",
            "minimum": 0
          },
          "welcome_message": {
            "type": "string"
          }
        }
      },
      "CreateSessionRequest": {
        "type": "object",
        "required": [
          "identifier",
          "password"
        ],
        "properties": {
          "identifier": {
            "type": "string",
            "maxLength": 320,
            "minLength": 1
          },
          "password": {
            "type": "string",
            "format": "password",
            "maxLength": 256,
            "minLength": 1
          }
        }
      },
      "CreateSessionResponse": {
        "type": "object",
        "required": [
          "token",
          "expires_in"
        ],
        "properties": {
          "expires_in": {
            "type": "integer",
            "format": "int64",
            "minimum": 0
          },
          "token": {
            "type": "string"
          }
        }
      },
      "CreateUserRequest": {
        "type": "object",
        "required": [
          "display_name",
          "email"
        ],
        "properties": {
          "avatar_url": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 2048,
            "minLength": 1
          },
          "display_name": {
            "type": "string",
            "maxLength": 128,
            "minLength": 1
          },
          "email": {
            "type": "string",
            "maxLength": 320,
            "minLength": 3
          },
          "metadata": {},
          "phone": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 32,
            "minLength": 1
          },
          "username": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 64,
            "minLength": 1
          }
        }
      },
      "ErrorResponseBody": {
        "type": "object",
        "description": "失败时的统一错误体（与 `AppError` 的序列化保持一致）。",
        "required": [
          "code",
          "message",
          "request_id"
        ],
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "details": {},
          "message": {
            "type": "string"
          },
          "request_id": {
            "type": "string"
          }
        }
      },
      "IntegrationsSettings": {
        "type": "object",
        "required": [
          "example_api_base",
          "example_api_key_is_set"
        ],
        "properties": {
          "example_api_base": {
            "type": "string"
          },
          "example_api_key_is_set": {
            "type": "boolean"
          }
        }
      },
      "PatchAppSettings": {
        "type": "object",
        "properties": {
          "check_interval_secs": {
            "type": [
              "integer",
              "null"
            ],
            "format": "int64",
            "minimum": 10
          },
          "welcome_message": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 256,
            "minLength": 1
          }
        }
      },
      "PatchCurrentUserPasswordRequest": {
        "type": "object",
        "required": [
          "current_password",
          "new_password"
        ],
        "properties": {
          "current_password": {
            "type": "string",
            "format": "password",
            "maxLength": 256,
            "minLength": 1
          },
          "new_password": {
            "type": "string",
            "format": "password",
            "maxLength": 256,
            "minLength": 8
          }
        }
      },
      "PatchCurrentUserRequest": {
        "type": "object",
        "properties": {
          "avatar_url": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 2048,
            "minLength": 1
          },
          "display_name": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 128,
            "minLength": 1
          },
          "email": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 320,
            "minLength": 3
          },
          "phone": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 32,
            "minLength": 1
          }
        },
        "additionalProperties": false
      },
      "PatchIntegrationsSettings": {
        "type": "object",
        "properties": {
          "example_api_base": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 2048,
            "minLength": 1
          },
          "example_api_key": {
            "type": [
              "string",
              "null"
            ],
            "format": "password",
            "description": "覆盖设置：留空字段表示“不修改”；若提供则必须非空。",
            "maxLength": 4096,
            "minLength": 1
          }
        }
      },
      "PatchSettingsRequest": {
        "type": "object",
        "properties": {
          "app": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/PatchAppSettings"
              }
            ]
          },
          "integrations": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/PatchIntegrationsSettings"
              }
            ]
          }
        }
      },
      "PatchUserRequest": {
        "type": "object",
        "properties": {
          "avatar_url": {
            "type": [
              "string",
              "null"
            ],
            "maxLength": 2048,
            "minLength": 1
          },
          "display_name": {
            "type": [
              "string",
