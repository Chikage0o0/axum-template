{
  "name": "project-name-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "gen:client": "orval --config orval.config.ts",
    "gen:schemas": "openapi-zod-client ../docs/openapi.json -o src/lib/api/generated/schemas.ts --export-schemas --template scripts/openapi-zod-schemas.hbs",
    "gen:api": "bun run gen:client && bun run gen:schemas",
    "gen:openapi": "bun run gen:api",
    "gen:openapi:zod": "bun run gen:schemas",
    "check:api-usage": "if rg -n \"apiFetchJson\\(|fetch\\(\" src --glob '!src/lib/api/generated/**' --glob '!src/lib/api/mutator.ts'; then echo '发现未授权的手写接口调用'; exit 1; fi",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.561.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/table-core": "^8.21.3",
    "bits-ui": "^2.14.4",
    "clsx": "^2.1.1",
    "embla-carousel-svelte": "^8.6.0",
    "formsnap": "^2.0.1",
    "layerchart": "2.0.0-next.43",
    "mode-watcher": "^1.1.0",
    "openapi-zod-client": "^1.18.3",
    "orval": "^8.2.0",
    "paneforge": "^1.0.2",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.48.2",
    "svelte-boring-avatars": "^1.2.6",
    "svelte-check": "^4.3.5",
    "svelte-sonner": "^1.0.7",
    "sveltekit-superforms": "^2.28.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "^7.3.1",
    "zod": "^4.3.6"
  }
}
