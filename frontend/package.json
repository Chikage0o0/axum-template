{
  "name": "project-name-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "gen:openapi": "openapi-typescript ../docs/openapi.json -o src/lib/api/generated/openapi.ts",
    "gen:openapi:zod": "openapi-zod-client ../docs/openapi.json -o src/lib/api/generated/openapi.zod.ts --export-schemas --template scripts/openapi-zod-schemas.hbs",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "openapi-typescript": "^7.10.1",
    "openapi-zod-client": "^1.18.3",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.48.2",
    "svelte-check": "^4.3.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "zod": "^4.3.6"
  }
}
